    "dev": "ts-node-dev --max_old_space_size=8144 start.ts",
    "dev-server": "./compile nuke ts-node-dev && npm run dev ",
    "cli": "ts-node cli.ts",
    "wh": "NODE_OPTIONS='--max-old-space-size=12288' ts-node wh.ts",
    "start": "NODE_OPTIONS='--max-old-space-size=8192' ts-node -T --files start.ts --",
    "init": "node ./scripts/install.js",
    "nuke": " ./compile nuke",
    "reset": " ./compile reset",
    "compile": " ./compile recompile",
    "old": " webpack --config ./webpack/old/.config.js",
